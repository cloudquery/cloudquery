{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/cloudquery/cloudquery/plugins/source/notion/client/spec",
  "$ref": "#/$defs/Spec",
  "$defs": {
    "Spec": {
      "properties": {
        "bearer_token": {
          "type": "string",
          "minLength": 1
        },
        "notion_version": {
          "type": "string",
          "minLength": 1,
          "default": "2022-02-22"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "bearer_token"
      ]
    }
  }
}
