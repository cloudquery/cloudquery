// Configuration AutoGenerated by CloudQuery CLI
cloudquery {
  provider "gcp" {
    source = "cloudquery/cq-provider-gcp"
    version = "latest"
  }
  // Can be configured via CLI variables
  connection {
    dsn = "postgres://postgres:pass@localhost:5432/postgres?sslmode=disable"
  }
}

// All Provider Configurations

provider "gcp" {
  configuration {
    // Optional. Filter as described https://cloud.google.com/sdk/gcloud/reference/projects/list --filter
    project_filter = ""
    // Optional. If not specified either using all projects accessible.
    // project_ids = [<CHANGE_THIS_TO_YOUR_PROJECT_ID>
  }
  resources = [
    "compute.addresses",
    "compute.autoscalers",
    "compute.disk_types",
    "compute.images",
    "compute.instances",
    "compute.interconnects",
    "compute.ssl_certificates",
    "compute.vpn_gateways",
    "iam.project_roles",
    "iam.service_accounts",
    "storage.buckets",
  ]
}


