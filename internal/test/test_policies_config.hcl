// Configuration AutoGenerated by CloudQuery CLI
cloudquery {

  provider "aws" {
    source  = "cloudquery/aws"
    version = "latest"
  }

  provider "gcp" {
    source  = "cloudquery/gcp"
    version = "latest"
  }

  provider "azure" {
    source  = "cloudquery/azure"
    version = "latest"
  }

  provider "k8s" {
    source  = "cloudquery/k8s"
    version = "latest"
  }

  // Can be configured via CLI variables
  connection {
    dsn = "host=localhost user=postgres password=pass database=postgres port=5432 sslmode=disable"
  }
}

// All Provider Configurations
provider "aws" {
  configuration {
    max_retries = 10
    max_backoff = 90
  }
  resources = [
    "*",
  ]
}

provider "gcp" {
  configuration {}
  resources = [
  "*"]
}

provider "azure" {
  configuration {}
  resources = [
  "*"]
}

provider "k8s" {
  configuration {}
  resources = [
  "*"]
}